{
  "name": "@darkobits/marin",
  "version": "0.1.1",
  "author": "darkobits",
  "description": "General-purpose dependency orchestration.",
  "license": "WTFPL",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "dependency",
    "orchestration"
  ],
  "scripts": {
    "clean": "del dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly && del **/*.spec.d.ts",
    "test": "jest",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline",
    "prebump": "run-p test build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta"
  },
  "dependencies": {
    "orchestrator": "~0.3.8",
    "ow": "~0.6.0",
    "toposort": "^2.0.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/node": "^7.0.0-beta.53",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-stage-3": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.0.0-beta.53",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "@types/orchestrator": "^0.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "del-cli": "^1.1.0",
    "jest": "^23.4.1",
    "npm-run-all": "^4.1.3",
    "standard-version": "^4.4.0",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.9.2"
  }
}
